<?xml version='1.0' encoding='ISO-8859-1'?>
<project>
    <pomVersion>3</pomVersion>
    <id>atlassian-profiling</id>
    <name>Atlassian Profiling</name>
    <currentVersion>1.1</currentVersion>
   <organization>
        <name>Atlassian Software Systems Pty Ltd</name>
        <logo>/images/profiling-logo.gif</logo>
        <url>http://opensource.atlassian.com/profiling</url>
    </organization>
    <inceptionYear>2002</inceptionYear>
    <package>com.pararede.profiling</package>
    <description>
        A simple framework for run-time profiling an application, focussed on J2EE web applications.
    </description>
    <shortDescription>Atlassian Profiling</shortDescription>
    <issueTrackingUrl>http://jira.atlassian.com/secure/BrowseProject.jspa?id=10481</issueTrackingUrl> 

    <developers>
        <developer>
            <name>Scott Farquhar</name>
            <id>scott</id>
            <email>scott@atlassian.com</email>
            <organization>Atlassian</organization>
            <roles>
                <role>Developer</role>
            </roles>
        </developer>
        <developer>
            <name>Mike Cannon-Brookes</name>
            <id>mike</id>
            <email>mike@atlassian.com</email>
            <organization>Atlassian</organization>
            <roles>
                <role>Developer</role>
            </roles>
        </developer>
    </developers>

    <dependencies>
        <dependency>
            <id>servletapi</id>
            <version>2.3</version>
        </dependency>
    </dependencies>
    <reports> 
      <report>maven-license-plugin</report> 
      <report>maven-faq-plugin</report> 
      <report>maven-pmd-plugin</report>
      <report>maven-simian-plugin</report> 
      <report>maven-jdepend-plugin</report> 
      <report>maven-jxr-plugin</report> 
      <report>maven-javadoc-plugin</report> 
      <!--
      <report>maven-changes-plugin</report>
      <report>maven-junit-report-plugin</report>
      <report>maven-clover-plugin</report> 
      -->
    </reports>

  <build>
    <nagEmailAddress>dev@atlassian.com</nagEmailAddress>
    <sourceDirectory>src/java</sourceDirectory>
